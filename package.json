{
  "name": "losbeekos",
  "version": "0.0.1",
  "description": "Personal blog of Jeroen van Beek, Dutch Front-End Developer.",
  "private": true,
  "engines": {
    "node": "^14.15.1",
    "npm": "^6.14.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/losbeekos/losbeekos.git"
  },
  "author": {
    "name": "Jeroen van Beek",
    "email": "losbeekos@gmail.com",
    "url": "https://www.losbeekos.nl"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/losbeekos/losbeekos/issues"
  },
  "scripts": {
    "build": "eleventy",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "stylelint": "stylelint 'src/**/*.css'"
  },
  "browserslist": [
    "> 1%",
    "not ie > 10",
    "not IE_Mob > 10",
    "not dead",
    "not OperaMini all"
  ],
  "prettier": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": true,
    "jsxBracketSameLine": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": false,
    "tabWidth": 2
  },
  "homepage": "https://github.com/losbeekos/losbeekos",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "cssnano": "^4.1.10",
    "eleventy-plugin-local-respimg": "^0.4.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.2",
    "markdown-it-anchor": "^6.0.1",
    "postcss": "^8.1.14",
    "postcss-import": "^13.0.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-property-sort-order-smacss": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-prettier": "^1.1.2"
  }
}
